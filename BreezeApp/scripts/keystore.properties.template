# IMPORTANT: Never commit keystore.properties to version control!

# Path to your keystore file (absolute or relative to project root)
storeFile=/Users/YOUR_USERNAME/Resource/android_key_mr

# Keystore password
storePassword=YOUR_KEYSTORE_PASSWORD

# Key alias
keyAlias=YOUR_KEY_ALIAS

# Key password
keyPassword=YOUR_KEY_PASSWORD
